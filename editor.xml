<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="JetBrains Mono" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern pattern="\boptimize\.b*" />
    <filter name="Optimization">
      <pattern index="1" />
    </filter>
  </component>
</application>